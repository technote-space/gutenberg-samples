{
  "name": "gutenberg-samples",
  "version": "0.1.6",
  "description": "gutenberg samples",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint src/**/**/*.js && eslint __tests__/**/**/*.js",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "yarn develop:dropdown1 && yarn develop:dropdown2",
    "build": "yarn build:dropdown1 && yarn build:dropdown2",
    "build:dropdown1": "webpack --mode production --config dropdown1.config.js",
    "build:dropdown2": "webpack --mode production --config dropdown2.config.js",
    "develop:dropdown1": "webpack --mode development --config dropdown1.config.js",
    "develop:dropdown2": "webpack --mode development --config dropdown2.config.js"
  },
  "author": "Technote <technote.space@gmail.com> (https://technote.space)",
  "license": "GPL-3.0",
  "dependencies": {
    "@technote-space/register-grouped-format-type": "^1.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@wordpress/block-editor": "^3.2.2",
    "@wordpress/block-library": "^2.9.2",
    "@wordpress/blocks": "^6.7.2",
    "@wordpress/components": "^8.3.2",
    "@wordpress/compose": "^3.7.2",
    "@wordpress/core-data": "^2.7.2",
    "@wordpress/data": "^4.9.2",
    "@wordpress/editor": "^9.7.2",
    "@wordpress/element": "^2.8.2",
    "@wordpress/hooks": "^2.6.0",
    "@wordpress/rich-text": "^3.7.2",
    "@wordpress/server-side-render": "^1.3.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "lodash": "^4.17.15",
    "mousetrap": "^1.6.3",
    "react-autosize-textarea": "^7.0.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
