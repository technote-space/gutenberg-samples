{
  "name": "gutenberg-samples",
  "version": "0.0.1",
  "description": "gutenberg samples",
  "scripts": {
    "test": "npm run lint && npm run cover",
    "lint": "eslint src/**/**/*.js && eslint test/**/**/*.js",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --recursive",
    "develop": "npm run develop:dropdown1 && npm run develop:dropdown2",
    "build": "npm run build:dropdown1 && npm run build:dropdown2",
    "build:dropdown1": "webpack --mode production --config dropdown1.config.js",
    "build:dropdown2": "webpack --mode production --config dropdown2.config.js",
    "develop:dropdown1": "webpack --mode development --config dropdown1.config.js",
    "develop:dropdown2": "webpack --mode development --config dropdown2.config.js"
  },
  "author": "Technote <technote.space@gmail.com> (https://technote.space)",
  "license": "GPL-3.0",
  "dependencies": {
    "@technote-space/register-grouped-format-type": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  }
}
