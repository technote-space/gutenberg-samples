{
  "name": "gutenberg-samples",
  "version": "0.1.7",
  "description": "gutenberg samples",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint src/**/**/*.js __tests__/**/**/*.js",
    "lint:fix": "eslint --fix src/**/**/*.ts __tests__/**/**/*.ts",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "yarn develop:dropdown1 && yarn develop:dropdown2",
    "build": "yarn build:dropdown1 && yarn build:dropdown2",
    "build:dropdown1": "webpack --mode production --config dropdown1.config.js",
    "build:dropdown2": "webpack --mode production --config dropdown2.config.js",
    "develop:dropdown1": "webpack --mode development --config dropdown1.config.js",
    "develop:dropdown2": "webpack --mode development --config dropdown2.config.js",
    "update": "ncu -u && yarn install && yarn upgrade && yarn audit"
  },
  "author": "Technote <technote.space@gmail.com> (https://technote.space)",
  "license": "GPL-3.0",
  "dependencies": {
    "@technote-space/register-grouped-format-type": "^1.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@wordpress/block-editor": "^3.5.0",
    "@wordpress/block-library": "^2.12.0",
    "@wordpress/blocks": "^6.10.0",
    "@wordpress/components": "^9.0.0",
    "@wordpress/compose": "^3.10.0",
    "@wordpress/core-data": "^2.10.0",
    "@wordpress/data": "^4.12.0",
    "@wordpress/editor": "^9.10.0",
    "@wordpress/element": "^2.10.0",
    "@wordpress/hooks": "^2.6.0",
    "@wordpress/rich-text": "^3.10.0",
    "@wordpress/server-side-render": "^1.6.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "lodash": "^4.17.15",
    "mousetrap": "^1.6.3",
    "react-autosize-textarea": "^7.0.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
