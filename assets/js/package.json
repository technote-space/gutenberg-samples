{
  "name": "gutenberg-samples",
  "version": "0.0.4",
  "description": "gutenberg samples",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint src/**/**/*.js && eslint __tests__/**/**/*.js",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "yarn develop:dropdown1 && yarn develop:dropdown2",
    "build": "yarn build:dropdown1 && yarn build:dropdown2",
    "build:dropdown1": "webpack --mode production --config dropdown1.config.js",
    "build:dropdown2": "webpack --mode production --config dropdown2.config.js",
    "develop:dropdown1": "webpack --mode development --config dropdown1.config.js",
    "develop:dropdown2": "webpack --mode development --config dropdown2.config.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "author": "Technote <technote.space@gmail.com> (https://technote.space)",
  "license": "GPL-3.0",
  "dependencies": {
    "@technote-space/register-grouped-format-type": "^1.0.21"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@wordpress/block-editor": "^2.2.0",
    "@wordpress/block-library": "^2.6.0",
    "@wordpress/blocks": "^6.4.0",
    "@wordpress/components": "^8.1.0",
    "@wordpress/compose": "^3.4.0",
    "@wordpress/core-data": "^2.4.0",
    "@wordpress/data": "^4.6.0",
    "@wordpress/editor": "^9.4.0",
    "@wordpress/element": "^2.5.0",
    "@wordpress/hooks": "^2.4.0",
    "@wordpress/rich-text": "^3.4.0",
    "@wordpress/server-side-render": "^1.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.5",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.2",
    "lodash": "^4.17.15",
    "mousetrap": "^1.6.3",
    "react-autosize-textarea": "^7.0.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  }
}
